EE_BIN = teapot.elf
EE_LIBS = -ldraw -lgraph -lmath3d -lmf -lpacket -ldma

all: teapot.c mesh_data.c
	$(EE_CC) -o $(EE_BIN) teapot.c $(EE_LIBS)
	$(EE_STRIP) --strip-all $(EE_BIN)

clean:
	rm -f $(EE_BIN)

run: $(EE_BIN)
	ps2client execee host:$(EE_BIN)

reset:
	ps2client reset

include $(PS2SDK)/samples/Makefile.pref
