EE_BIN = mc_example.elf
EE_LIBS = -lmc

all: mc_example.c
	$(EE_CC) -Wall -o $(EE_BIN) mc_example.c $(EE_LIBS)

clean:
	rm -f $(EE_BIN)

run: $(EE_BIN)
	ps2client execee host:$(EE_BIN)

reset:
	ps2client reset

include $(PS2SDK)/samples/Makefile.pref